<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="135.0" prefWidth="445.0" style="-fx-background-color: #E0ECF8;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bancolimpio.Resumen">
    <children>
        <Pane prefHeight="51.0" prefWidth="445.0" style="-fx-background-color: #3DAC53;">
            <children>
                <Label layoutX="14.0" layoutY="11.0" text="Emkharta" textFill="WHITE">
                    <font>
                        <Font name="Arial Black" size="20.0" />
                    </font>
                </Label>
                <ImageView fx:id="salir" fitHeight="29.0" fitWidth="54.0" layoutX="402.0" layoutY="11.0" onMouseClicked="#salir" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@salir.png" />
                    </image>
                </ImageView>
            </children>
        </Pane>
        <Label fx:id="cuenta" layoutX="14.0" layoutY="60.0" text="####-####-#" textFill="#808080">
            <font>
                <Font name="Arial" size="13.0" />
            </font>
        </Label>
        <Label fx:id="saldo" layoutX="113.0" layoutY="60.0" text="\$######.##" textFill="#808080">
            <font>
                <Font name="Arial" size="13.0" />
            </font>
        </Label>
        <Button fx:id="transferir" layoutX="51.0" layoutY="88.0" mnemonicParsing="false" onAction="#transferir" style="-fx-background-color: #FFFFFF;" text="Ingresar una palabra" />
        <Button fx:id="movimientos" layoutX="202.0" layoutY="88.0" mnemonicParsing="false" onAction="#ver" style="-fx-background-color: #FFFFFF;" text="Ver las palabras en el diccionario" />
    </children>
</AnchorPane>
